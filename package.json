{
  "name": "RecipieBox",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://vijayabharathib.github.io/fcc-project-react-recipies",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "coveralls": "^2.11.15",
    "extend-tape": "^1.2.0",
    "faucet": "0.0.1",
    "gh-pages": "^0.12.0",
    "npm-watch": "^0.1.7",
    "nyc": "^10.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-scripts": "0.8.4",
    "redux-devtools": "^3.3.1",
    "tape": "^4.6.3",
    "tape-jsx-equals": "^1.0.0"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0"
  },
  "watch": {
    "watch-test": "{src,test}/**/*.js"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "watch-test": "babel-node node_modules/.bin/tape test/**/*.test.js | faucet || true",
    "test": "babel-node node_modules/.bin/tape test/**/*.test.js | faucet",
    "test_original": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build",
    "report": "nyc npm test && nyc report --reporter=lcov",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "watch": "npm-watch"
  }
}
